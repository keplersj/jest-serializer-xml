{
  "name": "jest-serializer-xml",
  "version": "0.1.0",
  "description": "A snapshot serializer for XML strings.",
  "main": "index.js",
  "repository": "https://github.com/keplersj/jest-serializer-xml",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "is-xml": "^0.1.0",
    "xml-formatter": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.2.0",
    "@commitlint/config-conventional": "^6.1.3",
    "@commitlint/travis-cli": "^8.2.0",
    "eslint": "^6.5.1",
    "eslint-config-starstuff": "^1.0.0",
    "husky": "^0.14.3",
    "jest": "^24.9.0",
    "jest-runner-eslint": "^0.7.4",
    "jest-runner-prettier": "^0.3.1",
    "lint-staged": "^9.4.1",
    "prettier": "^1.18.2"
  },
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint *.js",
    "test": "jest",
    "precommit": "lint-staged"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": [
      "eslint-config-starstuff"
    ],
    "parserOptions": {
      "ecmaVersion": 6
    },
    "env": {
      "node": true
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "jest": {
    "projects": [
      {
        "displayName": "lint:prettier",
        "preset": "jest-runner-prettier"
      },
      {
        "displayName": "lint:eslint",
        "runner": "eslint",
        "testMatch": [
          "<rootDir>/**/*.js"
        ]
      }
    ]
  }
}
