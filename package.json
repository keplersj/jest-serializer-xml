{
  "name": "jest-serializer-xml",
  "version": "1.0.0",
  "description": "A snapshot serializer for XML strings.",
  "main": "index.js",
  "repository": "https://github.com/keplersj/jest-serializer-xml",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "is-xml": "^0.1.0",
    "xml-formatter": "^1.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "6.1.3",
    "@commitlint/config-conventional": "6.1.2",
    "@commitlint/travis-cli": "6.1.2",
    "eslint": "4.18.1",
    "eslint-config-kitchensink": "1.0.2",
    "husky": "0.14.3",
    "lint-staged": "6.1.1",
    "prettier": "1.11.1"
  },
  "scripts": {
    "commitmsg": "commitlint -e $GIT_PARAMS",
    "lint": "eslint *.js",
    "precommit": "lint-staged"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": [
      "kitchensink"
    ]
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "renovate": {
    "extends": [
      "config:base"
    ],
    "automerge": true,
    "automergeType": "branch-push",
    "major": {
      "automerge": false
    }
  }
}
