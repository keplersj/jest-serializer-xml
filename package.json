{
  "name": "jest-serializer-xml",
  "version": "0.0.0-development",
  "description": "A snapshot serializer for XML strings.",
  "main": "index.js",
  "repository": "https://github.com/keplersj/jest-serializer-xml",
  "author": "Kepler Sticka-Jones <kepler@stickajones.org>",
  "license": "MIT",
  "private": false,
  "dependencies": {
    "is-xml": "^0.1.0",
    "xml-formatter": "^2.0.1"
  },
  "devDependencies": {
    "@commitlint/cli": "^12.0.0",
    "@commitlint/config-conventional": "^12.0.0",
    "eslint": "^6.8.0",
    "eslint-config-starstuff": "^1.4.42",
    "husky": "^5.0.9",
    "jest": "^26.0.1",
    "jest-runner-eslint": "^0.10.0",
    "jest-runner-prettier": "^0.3.6",
    "lint-staged": "^10.1.7",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.7"
  },
  "scripts": {
    "test": "jest"
  },
  "commitlint": {
    "extends": [
      "@commitlint/config-conventional"
    ]
  },
  "eslintConfig": {
    "extends": "starstuff/auto",
    "env": {
      "node": true
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint"
    ]
  },
  "jest": {
    "projects": [
      {
        "displayName": "test"
      },
      {
        "displayName": "lint:prettier",
        "preset": "jest-runner-prettier"
      },
      {
        "displayName": "lint:eslint",
        "runner": "eslint",
        "testMatch": [
          "<rootDir>/**/*.js"
        ]
      }
    ]
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -e $HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged"
    }
  }
}
